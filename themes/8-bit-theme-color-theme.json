{
	"name": "8-bit-theme",
	"type": "dark",
	// DOCS: https://code.visualstudio.com/api/extension-guides/color-theme
	// ===================================================================
	// WORKBENCH COLORS - 8-BIT PALETTE
	// ===================================================================
	// Using retro 8-bit colors for the entire interface
	// Color palette inspired by classic NES/console games
	// ===================================================================
	
	"colors": {
		// === EDITOR - Main coding area ===
		"editor.background": "#000000",                    // Pure black (NES black)
		"editor.foreground": "#FFFFFF",                    // Pure white (NES white)
		"editor.lineHighlightBackground": "#000000",       // No highlight (keep black)
		"editor.selectionBackground": "#FFFF00",           // Yellow selection
		"editor.inactiveSelectionBackground": "#FF8800",   // Orange when unfocused
		"editor.selectionHighlightBackground": "#FF880055", // Orange with transparency
		"editor.findMatchBackground": "#FF8800",           // Orange for search matches
		"editor.findMatchHighlightBackground": "#FF880055", // Semi-transparent orange
		"editor.wordHighlightBackground": "#0000FF33",     // Semi-transparent blue
		"editor.wordHighlightStrongBackground": "#0000FF66", // Blue for write access
		"editor.hoverHighlightBackground": "#4C4C4C",      // Dark gray hover
		
		// === LINE NUMBERS ===
		"editorLineNumber.foreground": "#A0A0A0",          // Gray line numbers
		"editorLineNumber.activeForeground": "#FFFFFF",    // White active line number
		
		// === CURSOR ===
		"editorCursor.foreground": "#00FF00",              // Green cursor (NES green)
		"editorCursor.background": "#000000",              // Black cursor background
		
		// === INDENTATION GUIDES ===
		"editorIndentGuide.background": "#4C4C4C",         // Dark gray guides
		"editorIndentGuide.activeBackground": "#A0A0A0",   // Gray active guide
		
		// === WHITESPACE ===
		"editorWhitespace.foreground": "#4C4C4C",          // Dark gray for spaces/tabs
		
		// === RULERS ===
		"editorRuler.foreground": "#4C4C4C",               // Dark gray ruler lines
		
		// === BRACKETS ===
		"editorBracketMatch.background": "#0000A033",      // Dark blue with transparency
		"editorBracketMatch.border": "#0000FF",            // Blue bracket border
		"editorBracketPairGuide.activeBackground1": "#00FF00",
		"editorBracketPairGuide.activeBackground2": "#FFFF00",
		"editorBracketPairGuide.activeBackground3": "#0000FF",
		"editorBracketPairGuide.background1": "#4C4C4C",
		"editorBracketPairGuide.background2": "#4C4C4C",
		"editorBracketPairGuide.background3": "#4C4C4C",

		
		// === GUTTER (left side with line numbers) ===
		"editorGutter.background": "#000000",              // Black gutter
		"editorGutter.modifiedBackground": "#0000FF",      // Blue for modified lines
		"editorGutter.addedBackground": "#00FF00",         // Green for added lines
		"editorGutter.deletedBackground": "#FF0000",       // Red for deleted lines
		
		// === SIDEBAR - File explorer, search, etc ===
		"sideBar.background": "#4C4C4C",                   // Black sidebar
		"sideBar.foreground": "#FFFFFF",                   // White text
		"sideBar.border": "#4C4C4C",                       // Dark gray border
		"sideBarTitle.foreground": "#FFFF00",              // Yellow titles
		"sideBarSectionHeader.background": "#000000",      // Black section headers
		"sideBarSectionHeader.foreground": "#FFFF00",      // Yellow section text
		"sideBarSectionHeader.border": "#4C4C4C",          // Dark gray border
		
		// === ACTIVITY BAR - Far left icons (Explorer, Search, Git, etc) ===
		"activityBar.background": "#000000",               // Black activity bar
		"activityBar.foreground": "#FFFFFF",               // White icons
		"activityBar.inactiveForeground": "#A0A0A0",       // Gray inactive icons
		"activityBar.border": "#4C4C4C",                   // Dark gray border
		"activityBarBadge.background": "#FFFF00",          // Yellow notification badges
		"activityBarBadge.foreground": "#000000",          // Black text on badges
		
		// === STATUS BAR - Bottom bar with info ===
		"statusBar.background": "#0000A0",                 // Dark blue (NES dark blue)
		"statusBar.foreground": "#FFFFFF",                 // White text
		"statusBar.border": "#000000",                     // Dark gray border
		"statusBar.debuggingBackground": "#FF0000",        // Red when debugging
		"statusBar.debuggingForeground": "#FFFFFF",        // White text when debugging
		"statusBar.noFolderBackground": "#A000A0",         // Purple when no folder open
		"statusBarItem.prominentBackground": "#00FF00",    // Green for important items
		"statusBarItem.prominentForeground": "#000000",    // Black text on green
		
		// === TITLE BAR - Top window bar ===
		"titleBar.activeBackground": "#000000",            // Black when focused
		"titleBar.activeForeground": "#FFFFFF",            // White text when focused
		"titleBar.inactiveBackground": "#4C4C4C",          // Dark gray when unfocused
		"titleBar.inactiveForeground": "#A0A0A0",          // Gray text when unfocused
		"titleBar.border": "#4C4C4C",                      // Dark gray border
		
		// === TABS - Editor tabs ===
		"tab.activeBackground": "#000000",                 // Black active tab
		"tab.inactiveBackground": "#4C4C4C",               // Dark gray inactive tabs
		"tab.activeForeground": "#FFFF00",                 // Yellow text on active tab
		"tab.inactiveForeground": "#A0A0A0",               // Gray text on inactive tabs
		"tab.border": "#4C4C4C",                           // Dark gray tab borders
		"tab.activeBorder": "#FFFF00",                     // Yellow bottom border on active
		"tab.unfocusedActiveForeground": "#FFFFFF",        // White when unfocused but active
		"tab.unfocusedInactiveForeground": "#A0A0A0",      // Gray when unfocused & inactive
		
		// === EDITOR GROUPS - Split editors ===
		"editorGroup.border": "#4C4C4C",                   // Dark gray between splits
		"editorGroupHeader.tabsBackground": "#000000",     // Black tabs background
		"editorGroupHeader.noTabsBackground": "#000000",   // Black when no tabs
		
		// === TERMINAL - Integrated terminal ===
		"terminal.background": "#000000",                  // Black terminal background
		"terminal.foreground": "#FFFFFF",                  // White terminal text
		"terminal.ansiBlack": "#000000",                   // Black
		"terminal.ansiRed": "#FF0000",                     // Red (NES red)
		"terminal.ansiGreen": "#00FF00",                   // Green (NES green)
		"terminal.ansiYellow": "#FFFF00",                  // Yellow (NES yellow)
		"terminal.ansiBlue": "#0000FF",                    // Blue (NES blue)
		"terminal.ansiMagenta": "#A000A0",                 // Purple (NES purple)
		"terminal.ansiCyan": "#70A0FF",                    // Light blue (NES light blue)
		"terminal.ansiWhite": "#FFFFFF",                   // White
		"terminal.ansiBrightBlack": "#4C4C4C",             // Dark gray
		"terminal.ansiBrightRed": "#FF0000",               // Bright red
		"terminal.ansiBrightGreen": "#00FF00",             // Bright green
		"terminal.ansiBrightYellow": "#FFFF00",            // Bright yellow
		"terminal.ansiBrightBlue": "#70A0FF",              // Light blue
		"terminal.ansiBrightMagenta": "#FF80FF",           // Pink
		"terminal.ansiBrightCyan": "#70A0FF",              // Light blue
		"terminal.ansiBrightWhite": "#FFFFFF",             // White
		"terminalCursor.foreground": "#00FF00",            // Green cursor
		"terminalCursor.background": "#000000",            // Black cursor background
		
		// === PANEL - Bottom panel (Terminal, Problems, Output, Debug Console) ===
		"panel.background": "#000000",                     // Black panel
		"panel.border": "#4C4C4C",                         // Dark gray border
		"panelTitle.activeBorder": "#FFFF00",              // Yellow active border
		"panelTitle.activeForeground": "#FFFF00",          // Yellow active text
		"panelTitle.inactiveForeground": "#A0A0A0",        // Gray inactive text
		
		// === NOTIFICATIONS - Pop-up notifications ===
		"notificationCenter.border": "#4C4C4C",            // Dark gray border
		"notificationCenterHeader.background": "#000000",  // Black header
		"notificationCenterHeader.foreground": "#FFFFFF",  // White text
		"notifications.background": "#000000",             // Black notification
		"notifications.foreground": "#FFFFFF",             // White text
		"notifications.border": "#4C4C4C",                 // Dark gray border
		"notificationLink.foreground": "#70A0FF",          // Light blue links
		
		// === BUTTONS ===
		"button.background": "#0000FF",                    // Blue buttons (NES blue)
		"button.foreground": "#FFFFFF",                    // White text
		"button.hoverBackground": "#70A0FF",               // Light blue on hover
		
		// === INPUT FIELDS ===
		"input.background": "#000000",                     // Black input background
		"input.foreground": "#FFFFFF",                     // White input text
		"input.border": "#4C4C4C",                         // Dark gray border
		"input.placeholderForeground": "#A0A0A0",          // Gray placeholder
		"inputOption.activeBorder": "#0000FF",             // Blue when active
		"inputValidation.errorBackground": "#FF0000",      // Red for errors
		"inputValidation.errorForeground": "#FFFFFF",      // White error text
		"inputValidation.errorBorder": "#A00000",          // Dark red border
		"inputValidation.warningBackground": "#FF8800",    // Orange for warnings
		"inputValidation.warningForeground": "#000000",    // Black warning text
		"inputValidation.warningBorder": "#FF8800",        // Orange border
		"inputValidation.infoBackground": "#0000FF",       // Blue for info
		"inputValidation.infoForeground": "#FFFFFF",       // White info text
		"inputValidation.infoBorder": "#70A0FF",           // Light blue border
		
		// === DROPDOWN ===
		"dropdown.background": "#000000",                  // Black dropdown
		"dropdown.foreground": "#FFFFFF",                  // White text
		"dropdown.border": "#4C4C4C",                      // Dark gray border
		
		// === LISTS & TREES - File explorer, suggestions, etc ===
		"list.activeSelectionBackground": "#0000FF",       // Blue selected item
		"list.activeSelectionForeground": "#FFFFFF",       // White selected text
		"list.inactiveSelectionBackground": "#0000A0",     // Dark blue when unfocused
		"list.inactiveSelectionForeground": "#FFFFFF",     // White text
		"list.hoverBackground": "#4C4C4C",                 // Dark gray hover
		"list.hoverForeground": "#FFFFFF",                 // White hover text
		"list.focusBackground": "#0000FF",                 // Blue focus
		"list.focusForeground": "#FFFFFF",                 // White focus text
		"list.highlightForeground": "#FFFF00",             // Yellow for matches
		"list.errorForeground": "#FF0000",                 // Red for errors
		"list.warningForeground": "#FF8800",               // Orange for warnings
		
		// === SCROLLBAR ===
		"scrollbar.shadow": "#000000",                     // Black shadow
		"scrollbarSlider.background": "#4C4C4C88",         // Dark gray with transparency
		"scrollbarSlider.hoverBackground": "#A0A0A0BB",    // Gray on hover
		"scrollbarSlider.activeBackground": "#A0A0A0EE",   // Gray when dragging
		
		// === BADGES - Notification counts ===
		"badge.background": "#FF0000",                     // Red badges (NES red)
		"badge.foreground": "#FFFFFF",                     // White text on badges
		
		// === PROGRESS BAR ===
		"progressBar.background": "#00FF00",               // Green progress (NES green)
		
		// === PEEK VIEW - Definition preview window ===
		"peekView.border": "#0000FF",                      // Blue border
		"peekViewEditor.background": "#000000",            // Black editor area
		"peekViewEditor.matchHighlightBackground": "#FF880088", // Orange highlight
		"peekViewResult.background": "#000000",            // Black results area
		"peekViewResult.fileForeground": "#FFFFFF",        // White file names
		"peekViewResult.lineForeground": "#A0A0A0",        // Gray line text
		"peekViewResult.matchHighlightBackground": "#FF880088", // Orange matches
		"peekViewResult.selectionBackground": "#0000FF",   // Blue selection
		"peekViewResult.selectionForeground": "#FFFFFF",   // White selected text
		"peekViewTitle.background": "#000000",             // Black title area
		"peekViewTitleDescription.foreground": "#A0A0A0",  // Gray description
		"peekViewTitleLabel.foreground": "#FFFF00",        // Yellow title
		
		// === MERGE CONFLICTS ===
		"merge.currentHeaderBackground": "#00FF0044",      // Green for current
		"merge.currentContentBackground": "#00FF0022",     // Light green content
		"merge.incomingHeaderBackground": "#0000FF44",     // Blue for incoming
		"merge.incomingContentBackground": "#0000FF22",    // Light blue content
		"merge.border": "#4C4C4C",                         // Dark gray border
		
		// === GIT DECORATIONS - File colors in explorer ===
		"gitDecoration.modifiedResourceForeground": "#0000FF",    // Blue for modified
		"gitDecoration.deletedResourceForeground": "#FF0000",     // Red for deleted
		"gitDecoration.untrackedResourceForeground": "#00FF00",   // Green for untracked
		"gitDecoration.ignoredResourceForeground": "#A0A0A0",     // Gray for ignored
		"gitDecoration.conflictingResourceForeground": "#FF8800", // Orange for conflicts
		
		// === BREADCRUMBS - File path navigation ===
		"breadcrumb.foreground": "#A0A0A0",                // Gray breadcrumbs
		"breadcrumb.background": "#000000",                // Black background
		"breadcrumb.focusForeground": "#FFFFFF",           // White when focused
		"breadcrumb.activeSelectionForeground": "#FFFF00", // Yellow when selected
		"breadcrumbPicker.background": "#000000",          // Black picker background
		
		// === MINIMAP - Code overview on right side ===
		"minimap.background": "#000000",                   // Black minimap
		"minimap.findMatchHighlight": "#FF8800",           // Orange for matches
		"minimap.selectionHighlight": "#FFFF00",           // Yellow for selection
		"minimap.errorHighlight": "#FF0000",               // Red for errors
		"minimap.warningHighlight": "#FF8800",             // Orange for warnings
		
		// === OVERVIEW RULER - Right side colored marks ===
		"editorOverviewRuler.border": "#4C4C4C",           // Dark gray border
		"editorOverviewRuler.modifiedForeground": "#0000FF", // Blue for modified
		"editorOverviewRuler.addedForeground": "#00FF00",    // Green for added
		"editorOverviewRuler.deletedForeground": "#FF0000",  // Red for deleted
		"editorOverviewRuler.errorForeground": "#FF0000",    // Red for errors
		"editorOverviewRuler.warningForeground": "#FF8800",  // Orange for warnings
		"editorOverviewRuler.findMatchForeground": "#FF8800", // Orange for matches
		
		// === PROBLEMS PANEL - Errors and warnings ===
		"problemsErrorIcon.foreground": "#FF0000",         // Red error icon
		"problemsWarningIcon.foreground": "#FF8800",       // Orange warning icon
		"problemsInfoIcon.foreground": "#0000FF",          // Blue info icon
		
		// === DIFF EDITOR - Side-by-side comparison ===
		"diffEditor.insertedTextBackground": "#00FF0033", // Green for added
		"diffEditor.removedTextBackground": "#FF000033",  // Red for removed
		"diffEditor.insertedTextBorder": "#00FF00",       // Green border
		"diffEditor.removedTextBorder": "#FF0000",        // Red border
		"diffEditor.border": "#4C4C4C"                    // Dark gray border
	},
	
	// ===================================================================
	// TOKEN COLORS - SYNTAX HIGHLIGHTING
	// ===================================================================
	// These rules define how code elements are colored
	// Works for: JavaScript, TypeScript, React, Node, Python, Java, C, CSS, HTML, Markdown
	// ===================================================================
	
	"tokenColors": [
		// ===================================================================
		// MARKDOWN & DOCUMENTATION
		// ===================================================================
		
		{
			// Italic text in Markdown (*text* or _text_)
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			// Bold text in Markdown (**text** or __text__)
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			// Git diff headers (e.g., diff --git a/file.js b/file.js)
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#0000A0" // Dark blue
			}
		},
		
		// ===================================================================
		// COMMENTS - All languages
		// ===================================================================
		
		{
			// ALL comments: //, /* */, /** JSDoc */, #, <!-- -->
			// JavaScript: // comment, /* comment */
			// Python: # comment
			// Java: // comment, /* comment */
			// C: // comment, /* comment */
			// HTML: <!-- comment -->
			// CSS: /* comment */
			"scope": "comment",
			"settings": {
				"foreground": "#A0A0A0", // Gray (NES gray)
				"fontStyle": "italic"
			}
		},
		{
			// JSDoc tags: @param, @returns, @type, etc
			"scope": [
				"storage.type.class.jsdoc",
				"entity.name.type.instance.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#70A0FF" // Light blue
			}
		},
		
		// ===================================================================
		// CONSTANTS & LITERAL VALUES - All languages
		// ===================================================================
		
		{
			// Special values: true, false, null, undefined, NaN, Infinity
			// JavaScript/TypeScript: true, false, null, undefined
			// Python: True, False, None
			// Java: true, false, null
			// C: true, false, NULL
			"scope": "constant.language",
			"settings": {
				"foreground": "#FF0000" // Red (NES red)
			}
		},
		{
			// Numbers and RGB colors
			// All languages: 42, 3.14, 0xFF, 0b1010
			// CSS: #ff0000, rgb(255, 0, 0)
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#FF80FF" // Pink (NES pink)
			}
		},
		{
			// Regular expressions (regex)
			// JavaScript: /[a-z]+/gi, new RegExp('\\d+')
			// Python: re.compile(r'\d+')
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#A000A0" // Purple (NES purple)
			}
		},
		{
			// Character constants
			// C: 'a', '\n', '\0'
			// Java: 'x'
			"scope": "constant.character",
			"settings": {
				"foreground": "#FF80FF" // Pink
			}
		},
		
		// ===================================================================
		// HTML/JSX TAGS
		// ===================================================================
		
		{
			// HTML/JSX tag names: <div>, <span>, <button>, <MyComponent>
			// HTML: <div>, <p>, <section>
			// JSX/React: <MyComponent>, <Button>
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#FF8800" // Orange (NES orange)
			}
		},
		{
			// CSS tag selectors
			// CSS: div { ... }, p { ... }, section { ... }
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#FFFF00" // Yellow (NES yellow)
			}
		},
		
		// ===================================================================
		// HTML/JSX ATTRIBUTES
		// ===================================================================
		
		{
			// HTML/JSX attributes and props
			// HTML: <div class="..." id="..." disabled>
			// JSX: <Component onClick={...} className="..." disabled>
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#70A0FF", // Light blue (NES light blue)
				"fontStyle": "italic"
			}
		},
		{
			// CSS class and ID selectors
			// CSS: .className, #id, [data-attribute]
			"scope": [
				"source.css entity.other.attribute-name",
				"source.css.less entity.other.attribute-name.id",
				"source.scss entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#FFFF00" // Yellow
			}
		},
		
		// ===================================================================
		// ERRORS & WARNINGS
		// ===================================================================
		
		{
			// Invalid/syntax error code
			// All languages: highlighted by VS Code when there's a syntax error
			"scope": "invalid",
			"settings": {
				"foreground": "#FF0000" // Red (NES red)
			}
		},
		{
			// Deprecated code
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#A00000", // Dark red
				"fontStyle": "strikethrough"
			}
		},
		
		// ===================================================================
		// MARKDOWN - FORMATTING
		// ===================================================================
		
		{
			// Underlined text in Markdown
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			// Bold text in Markdown
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF" // White for emphasis
			}
		},
		{
			// Headers in Markdown: # Title, ## Subtitle
			"scope": "markup.heading",
			"settings": {
				"foreground": "#FFFF00", // Yellow
				"fontStyle": "bold"
			}
		},
		{
			// Italic text in Markdown
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			// Inline code in Markdown: `code`
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#00FF00" // Green
			}
		},
		{
			// Code blocks in Markdown: ```code```
			"scope": "markup.fenced_code.block",
			"settings": {
				"foreground": "#00FF00" // Green
			}
		},
		{
			// Links in Markdown: [text](url)
			"scope": [
				"markup.underline.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#70A0FF", // Light blue
				"fontStyle": "underline"
			}
		},
		{
			// Lists in Markdown: -, *, +, 1.
			"scope": "markup.list",
			"settings": {
				"foreground": "#FF8800" // Orange
			}
		},
		{
			// Quotes in Markdown: > quote
			"scope": "markup.quote",
			"settings": {
				"foreground": "#A0A0A0", // Gray
				"fontStyle": "italic"
			}
		},
		
		// ===================================================================
		// GIT DIFF - CHANGES
		// ===================================================================
		
		{
			// Lines ADDED in Git diffs (+ green lines)
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#00FF00" // Green (NES green)
			}
		},
		{
			// Lines REMOVED in Git diffs (- red lines)
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#FF0000" // Red (NES red)
			}
		},
		{
			// Lines MODIFIED in Git diffs
			"scope": "markup.changed",
			"settings": {
				"foreground": "#0000FF" // Blue (NES blue)
			}
		},
		
		// ===================================================================
		// CSS - SELECTORS
		// ===================================================================
		
		{
			// CSS selectors: .class, #id, element
			"scope": "meta.selector",
			"settings": {
				"foreground": "#FFFF00" // Yellow
			}
		},
		{
			// HTML/JSX tag brackets: < > </ />
			// Example: <div>...</div>
			//          ^   ^   ^^    ^^
			"name": "Brackets of XML/HTML tags",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#A0A0A0" // Gray
			}
		},
		{
			// CSS pseudo-classes: :hover, :active, :focus
			"scope": "entity.other.attribute-name.pseudo-class.css",
			"settings": {
				"foreground": "#FF8800" // Orange
			}
		},
		{
			// CSS pseudo-elements: ::before, ::after
			"scope": "entity.other.attribute-name.pseudo-element.css",
			"settings": {
				"foreground": "#FF8800" // Orange
			}
		},
		
		// ===================================================================
		// PREPROCESSORS (C/C++)
		// ===================================================================
		
		{
			// Preprocessor directives: #include, #define, #ifdef
			// C/C++: #include <stdio.h>, #define MAX 100
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#A000A0" // Purple
			}
		},
		{
			// Strings in preprocessor directives
			// C/C++: #include "myfile.h"
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#FFFFFF" // White
			}
		},
		{
			// Numbers in preprocessor directives
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#FF80FF" // Pink
			}
		},
		
		// ===================================================================
		// PYTHON - SPECIFIC
		// ===================================================================
		
		{
			// Python dictionary keys
			// Python: my_dict = {"key": "value"}
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#70A0FF" // Light blue
			}
		},
